{
  "name": "nebulaforge-x",
  "version": "1.0.0",
  "description": "NebulaForge X - AI-Powered 3D Game Engine",
  "main": "dist/index.js",
  "scripts": {
    "dev": "concurrently \"npm run dev:frontend\" \"npm run dev:backend\"",
    "dev:frontend": "vite",
    "dev:backend": "cd backend && python -m uvicorn main:app --reload --host 0.0.0.0 --port 8000",
    "build": "npm run build:frontend && npm run build:backend",
    "build:frontend": "vite build",
    "build:backend": "cd backend && python -m pip install -r requirements.txt",
    "build:mobile": "npm run build:frontend && npx cap sync",
    "build:android": "npm run build:mobile && npx cap build android",
    "build:ios": "npm run build:mobile && npx cap build ios",
    "preview": "vite preview",
    "test": "vitest",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "format": "prettier --write \"src/**/*.{ts,tsx,js,jsx,css,scss}\"",
    "install:all": "npm install && cd backend && pip install -r requirements.txt",
    "mobile:init": "npx cap init",
    "mobile:add-android": "npx cap add android",
    "mobile:add-ios": "npx cap add ios",
    "mobile:serve": "npx cap serve",
    "export:web": "npm run build:frontend && npm run package:web",
    "export:android": "npm run build:android",
    "export:desktop": "npm run build:frontend && npm run package:desktop",
    "package:web": "cd dist && zip -r ../nebulaforge-web.zip .",
    "package:desktop": "electron-builder"
  },
  "dependencies": {
    "@types/three": "^0.158.3",
    "three": "^0.158.0",
    "@react-three/fiber": "^8.15.12",
    "@react-three/drei": "^9.92.7",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.20.1",
    "framer-motion": "^10.16.16",
    "zustand": "^4.4.7",
    "axios": "^1.6.2",
    "socket.io-client": "^4.7.4",
    "@monaco-editor/react": "^4.6.0",
    "codemirror": "^6.0.1",
    "@uiw/react-codemirror": "^4.21.21",
    "react-flow-renderer": "^10.3.17",
    "reactflow": "^11.10.1",
    "styled-components": "^6.1.6",
    "lucide-react": "^0.294.0",
    "react-hot-toast": "^2.4.1",
    "idb": "^7.1.1",
    "comlink": "^4.4.1"
  },
  "devDependencies": {
    "@types/react": "^18.2.43",
    "@types/react-dom": "^18.2.17",
    "@typescript-eslint/eslint-plugin": "^6.14.0",
    "@typescript-eslint/parser": "^6.14.0",
    "@vitejs/plugin-react": "^4.2.1",
    "eslint": "^8.55.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "typescript": "^5.2.2",
    "vite": "^5.0.8",
    "vite-plugin-pwa": "^0.17.4",
    "vitest": "^1.0.4",
    "prettier": "^3.1.1",
    "concurrently": "^8.2.2",
    "@capacitor/core": "^5.5.1",
    "@capacitor/cli": "^5.5.1",
    "@capacitor/android": "^5.5.1",
    "@capacitor/ios": "^5.5.1",
    "electron": "^27.1.3",
    "electron-builder": "^24.6.4"
  },
  "keywords": [
    "game-engine",
    "3d",
    "ai",
    "typescript",
    "python",
    "rust",
    "webgl",
    "pwa",
    "mobile",
    "android",
    "ios"
  ],
  "author": "NebulaForge Team",
  "license": "MIT"
}